{
  "builds": [
    {
      "src": "frontend",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://qrsona.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}